import java.util.*;
class even_odd
{
	public static void main(String ar[])
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a number ");
		int n=sc.nextInt();
		if((n&1)!=1)
		{
			System.out.println("even");
		}
		else
		{
			System.out.println("odd");
		}
	}
} 


o/p 1:
Enter a number
34
even

o/p 2:
Enter a number
45
odd
